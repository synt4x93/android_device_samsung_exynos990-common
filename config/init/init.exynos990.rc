on late-fs
    wait_for_prop hwservicemanager.ready true
    mount_all /vendor/etc/fstab.${ro.hardware} --late

on fs
    mount_all /vendor/etc/fstab.${ro.hardware} --early

    wait /dev/block/platform/13100000.ufs/by-name/sec_efs
    mount ext4 /dev/block/platform/13100000.ufs/by-name/sec_efs /efs nosuid nodev noatime noauto_da_alloc,discard,journal_checksum,data=ordered,errors=panic    
    setprop ro.crypto.fde_algorithm AES-256-XTS-DISK

    wait /dev/block/platform/13100000.ufs/by-name/spu
    mount ext4 /dev/block/platform/13100000.ufs/by-name/spu /spu nosuid nodev noatime noauto_da_alloc,discard,journal_checksum,data=ordered,errors=panic

    restorecon_recursive /mnt/vendor/efs
    restorecon_recursive /mnt/vendor/cpefs
    restorecon_recursive /mnt/vendor/persist

    chown radio system /mnt/vendor/efs
    chmod 0771 /mnt/vendor/efs
    chown radio system /mnt/vendor/cpefs
    chmod 0771 /mnt/vendor/cpefs
    chown system system /mnt/vendor/persist

    mkdir /mnt/vendor/persist/data/sfs 0700 system system
    mkdir /mnt/vendor/persist/data/tz 0700 system system

    wait /dev/block/platform/13100000.ufs/by-name/dqmdbg
    mount ext4 /dev/block/platform/13100000.ufs/by-name/dqmdbg /dqmdbg nosuid nodev noatime noauto_da_alloc,discard,journal_checksum,data=ordered,errors=panic

    chown system system /dqmdbg
    chmod 0770 /dqmdbg
    restorecon /dqmdbg

    # Enable rmnet data and aggregation
    setprop persist.rmnet.mux enabled
    setprop persist.rmnet.data.enable true
    setprop persist.data.wda.enable true
    setprop persist.data.df.agg.dl_pkt 10
    setprop persist.data.df.agg.dl_size 4096

# Permissions for ION
    chmod 0660 /sys/class/ion_cma/ion_video_ext/isolated
    chown system system /sys/class/ion_cma/ion_video_ext/isolated

# Permissions for backlight
